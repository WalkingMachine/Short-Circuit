<?xml version="1.0"?>
<robot name="SARA">
<!-- LINKS -->
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.65 0.78 0.14"/>
            </geometry>
            <origin xyz="0 -0.2 0.07" rpy="0 0 0"/>
            <material name="grey">
                <color rgba="0.62 0.62 0.62 1"/>
            </material>
        </visual>
        <visual>
            <geometry>
                <box size="0.65 0.55 0.70"/>
            </geometry>
            <origin xyz="0 -0.315 0.42" rpy="0 0 0"/>
        </visual>
        <visual>
            <geometry>
                <cylinder length="0.7" radius="0.08"/>
            </geometry>
            <origin xyz="0 0 0.49" rpy="0 0 0"/>
        </visual>
    </link>

    <link name="laser_link">
        <visual>
            <geometry>
                <cylinder length="0.1" radius="0.03"/>
            </geometry>
            <origin xyz="0 0 0.05" rpy="0 0 0"/>
            <material name="orange">
                <color rgba="1 0.5 0 1"/>
            </material>
        </visual>
    </link>

    <link name="torso_link">
        <visual>
            <geometry>
                <box size="0.4 0.4 0.2"/>
            </geometry>
            <origin xyz="0 0 0.1" rpy="0 0 0"/>
            <material name="black">
                <color rgba="0.12 0.12 0.12 1"/>
            </material>
        </visual>
        <visual>
            <geometry>
                <box size="0.8 0.2 0.2"/>
            </geometry>
            <origin xyz="0 -0.1 0.1" rpy="0 0 0"/>
        </visual>
        <visual>
            <geometry>
                <sphere radius="0.2"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </visual>
        <visual>
            <geometry>
                <cylinder length="0.5" radius="0.08"/>
            </geometry>
            <origin xyz="0 0 -0.4" rpy="0 0 0"/>
            <material name="grey"/>
        </visual>
    </link>

    <link name="head_link">
        <visual>
            <geometry>
                <box size="0.25 0.1 0.4"/>
            </geometry>
            <origin xyz="0 0.15 0" rpy="0 0 0"/>
            <material name="red">
                <color rgba="1 0 0 1"/>
            </material>
        </visual>
        <visual>
            <geometry>
                <sphere radius="0.2"/>
            </geometry>
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </visual>
    </link>

    <link name="neck_link">
         <visual>
             <geometry>
                 <box size="0.2 0.2 0.2"/>
             </geometry>
             <origin xyz="0 0 0.1" rpy="0 0 0"/>
             <material name="grey"/>
        </visual>
    </link>

    <link name="clavicule_link">
         <visual>
             <geometry>
                 <cylinder length="0.1" radius="0.06"/>
             </geometry>
             <origin xyz="0 0 0.05" rpy="0 0 0"/>
             <material name="grey"/>
         </visual>
        <visual>
            <geometry>
                <cylinder length="0.15" radius="0.06"/>
            </geometry>
            <origin xyz="0 0.05 0.1" rpy="-1.57 0 0"/>
        </visual>
    </link>

    <link name="fixUpperArm_link">
         <visual>
            <geometry>
                <cylinder length="0.1" radius="0.06"/>
            </geometry>
            <origin xyz="0 0 -0.05" rpy="0 0 0"/>
             <material name="grey"/>
        </visual>
        <visual>
            <geometry>
                <cylinder length="0.15" radius="0.06"/>
            </geometry>
            <origin xyz="0 0.05 0" rpy="-1.57 0 0"/>
        </visual>
    </link>

    <link name="rotUpperArm_link">
        <!-- <visual>
            <geometry>
                <cylinder length="0.6" radius="0.1"/>
            </geometry>
            <origin xyz="0 0 -0.3" rpy="0 0 0"/>
        </visual> -->
    </link>

    <link name="fixLowerArm_link">
        <!-- <visual>
            <geometry>
                <cylinder length="0.6" radius="0.1"/>
            </geometry>
            <origin xyz="0 0 -0.3" rpy="0 0 0"/>
        </visual> -->
    </link>

    <link name="rotLowerArm_link">
        <!-- <visual>
            <geometry>
                <cylinder length="0.6" radius="0.1"/>
            </geometry>
            <origin xyz="0 0 -0.3" rpy="0 0 0"/>
        </visual> -->
    </link>

    <link name="gripper_link">
        <!--<visual>
            <geometry>
                <cylinder length="0.6" radius="0.1"/>
            </geometry>
            <origin xyz="0 0 -0.3" rpy="0 0 0"/>
        </visual> -->
    </link>

  <!-- JOINTS -->

    <joint name="laser_joint" type="fixed">
        <parent link="base_link"/>
        <child link="laser_link"/>
        <origin xyz="0 0.14 0.14" rpy="0 0 0" />
    </joint>

    <joint name="spine_joint" type="prismatic">
        <parent link="base_link"/>
        <child link="torso_link"/>
        <limit effort="1000.0" lower="1" upper="1.5" velocity="0.2"/>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 0 1.4" />
    </joint>

    <joint name="sternoclavicular_joint" type="revolute">
        <parent link="torso_link"/>
        <child link="clavicule_link"/>
        <limit effort="1000.0" lower="-1.57" upper="1.57" velocity="0.5"/>
        <origin xyz="0.3 0 0.1" rpy="-1.57 -1.57 0" />
        <axis xyz="0 0 1" />
    </joint>

    <joint name="shoulder_joint" type="revolute">
        <parent link="clavicule_link"/>
        <child link="fixUpperArm_link"/>
        <limit effort="1000.0" lower="-3.1416" upper="3.1416" velocity="0.5"/>
        <origin xyz="0 0.23 0.1" rpy="-1.57 1.57 0" />
        <axis xyz="0 0 1" />
    </joint>

    <joint name="shoulderRot_joint" type="revolute">
        <parent link="fixUpperArm_link"/>
        <child link="rotUpperArm_link"/>
        <limit effort="1000.0" lower="-3.1416" upper="3.1416" velocity="0.5"/>
        <origin xyz="0 0.2 0" rpy="-1.57 0 0" />
        <axis xyz="0 0 1" />
    </joint>

    <joint name="elbow_joint" type="revolute">
        <parent link="rotUpperArm_link"/>
        <child link="fixLowerArm_link"/>
        <limit effort="1000.0" lower="0.0" upper="2" velocity="0.5"/>
        <origin xyz="0 0 0.2" rpy="1.57 0 0" />
        <axis xyz="0 0 1" />
    </joint>

    <joint name="WristRot_joint" type="revolute">
        <parent link="fixLowerArm_link"/>
        <child link="rotLowerArm_link"/>
        <limit effort="1000.0" lower="-3.1416" upper="3.1416" velocity="0.5"/>
        <origin xyz="0 0.2 0" rpy="-1.57 0 0" />
        <axis xyz="0 0 1" />
    </joint>

    <joint name="Wrist_joint" type="fixed">
        <parent link="rotLowerArm_link"/>
        <child link="gripper_link"/>
        <origin xyz="0 0 0.2" rpy="0 0 0" />
    </joint>

    <joint name="torsoNeck_joint" type="revolute">
        <parent link="torso_link"/>
        <child link="neck_link"/>
        <limit effort="1000.0" lower="-3.1416" upper="3.1416" velocity="0.5"/>
        <origin xyz="0 0 0.2" rpy="0 0 0" />
        <axis xyz="0 0 1" />
    </joint>

    <joint name="neckHead_joint" type="revolute">
        <parent link="neck_link"/>
        <child link="head_link"/>
        <limit effort="1000.0" lower="-1.04" upper="1.04" velocity="0.5"/>
        <origin xyz="0 0 0.2" rpy="0 1.57 0" />
        <axis xyz="0 0 1" />
    </joint>
</robot>